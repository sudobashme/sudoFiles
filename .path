if [[ "$PATH" != *":/usr/local/sbin:"* ]]; then
    PATH=/usr/local/sbin:$PATH
fi
if [[ "$PATH" != *":/usr/local/bin:"* ]]; then
    PATH=/usr/local/bin:$PATH
fi
if [[ "$PATH" != *":/Users/slime/go/bin:"* ]]; then
    PATH=/Users/slime/go/bin:$PATH
fi

# and export it
export PATH