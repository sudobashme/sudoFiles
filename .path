if [[ "$PATH" != *":/usr/local/sbin:"* ]]; then
    PATH=/usr/local/sbin:$PATH
fi
if [[ "$PATH" != *":/usr/local/bin:"* ]]; then
    PATH=/usr/local/bin:$PATH
fi
if [[ "$PATH" != *":/usr/local/opt/go/libexec/bin:"* ]]; then
    PATH=/usr/local/opt/go/libexec/bin:$PATH
fi
if [[ "$PATH" != *":/Users/slime/go/bin:"* ]]; then
    PATH=/Users/slime/go/bin:$PATH
fi
if [[ "$PATH" != *":/Users/slime/.rvm/bin:"* ]]; then
    PATH=/Users/slime/.rvm/bin:$PATH
fi
if [[ "$PATH" != *":/Applications/Visual Studio Code.app/Contents/Resources/app/bin:"* ]]; then
    PATH=/Applications/Visual\ Studio\ Code.app/Contents/Resources/app/bin:$PATH
fi

export PATH
